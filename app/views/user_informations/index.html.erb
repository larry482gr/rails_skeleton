<p id="notice"><%= notice %></p>

<h1>Listing User Informations</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Gender</th>
      <th>Fname</th>
      <th>Lname</th>
      <th>Mobile</th>
      <th>Photo</th>
      <th>Addresses</th>
      <th>City</th>
      <th>Region</th>
      <th>Zipcode</th>
      <th>Country</th>
      <th>Failed attempts</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_informations.each do |user_information| %>
      <tr>
        <td><%= user_information.user %></td>
        <td><%= user_information.gender %></td>
        <td><%= user_information.fname %></td>
        <td><%= user_information.lname %></td>
        <td><%= user_information.mobile %></td>
        <td><%= user_information.photo %></td>
        <td><%= user_information.addresses %></td>
        <td><%= user_information.city %></td>
        <td><%= user_information.region %></td>
        <td><%= user_information.zipcode %></td>
        <td><%= user_information.country %></td>
        <td><%= user_information.failed_attempts %></td>
        <td><%= user_information.created_by %></td>
        <td><%= link_to 'Show', user_information %></td>
        <td><%= link_to 'Edit', edit_user_information_path(user_information) %></td>
        <td><%= link_to 'Destroy', user_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User information', new_user_information_path %>
