<p id="notice"><%= notice %></p>

<h1>Listing User Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Gender</th>
      <th>Fname</th>
      <th>Lname</th>
      <th>Mobile</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.gender %></td>
        <td><%= user_profile.fname %></td>
        <td><%= user_profile.lname %></td>
        <td><%= user_profile.mobile %></td>
        <td><%= user_profile.photo %></td>
        <td><%= link_to 'Show', user_profile_path(user_profile.user_id) %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(user_profile.user_id) %></td>
        <td><%= link_to 'Destroy', user_profile_path(user_profile.user_id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>